datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model SensorEvent {
  createdAt DateTime  @default(now())
  deviceId  String?
  eventData Json?
  eventType String?
  id        String    @id @default(cuid())
  timestamp DateTime?
  updatedAt DateTime  @updatedAt
}
